{
  "name": "resume-constructor",
  "version": "1.0.0",
  "description": "A TypeScript application designed to create software-engineering resumes based on principles from the book The Tech Resume Inside Out.",
  "main": "index.tsx",
  "type": "module",
  "scripts": {
    "test": "jest --passWithNoTests",
    "test:ci": "jest --passWithNoTests --watchAll=false",
    "e2e": "docker run --rm --network host -v $(pwd):/work/ -w /work/ -it mcr.microsoft.com/playwright:v1.57.0-noble npx playwright test",
    "e2e:update": "docker run --rm --network host -v $(pwd):/work/ -w /work/ -it mcr.microsoft.com/playwright:v1.57.0-noble npx playwright test --update-snapshots",
    "start-g": "webpack serve --open-app-name 'google-chrome' --config webpack.dev.cjs",
    "start": "webpack serve --config webpack.dev.cjs",
    "build": "webpack --config webpack.prod.cjs",
    "p": "git add . && git commit && git push origin main",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "Mikhail Sholokhov",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@eslint/js": "^9.33.0",
    "@eslint/json": "^0.13.1",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/babel__core": "^7.20.5",
    "@types/babel__preset-env": "^7.10.0",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/identity-obj-proxy": "^3.0.2",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.20",
    "@types/node": "^22.7.5",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@types/react-test-renderer": "^19.1.0",
    "@types/sass-loader": "^8.0.9",
    "@types/webpack": "^5.28.5",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^29.7.0",
    "babel-loader": "^10.0.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "canvas": "^3.2.0",
    "clean-webpack-plugin": "^4.0.0",
    "clsx": "^2.1.1",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.21.3",
    "eslint-plugin-perfectionist": "^4.14.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-you-might-not-need-an-effect": "^0.7.0",

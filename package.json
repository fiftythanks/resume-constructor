    "webpack-dev-server": "^5.2.0",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@react-pdf/renderer": "^4.3.0",
    "@vercel/speed-insights": "^1.2.0",
    "immer": "^10.1.1",
    "pdfjs-dist": "^5.4.54",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "use-immer": "^0.11.0"
  },
  "lint-staged": {
    "src/**/*.{mjs,js,jsx,mts,ts,tsx}": [
      "eslint --fix",
      "prettier --write --ignore-unknown",
      "jest --bail --findRelatedTests --passWithNoTests"
    ],
    "src/**/*.{html,json}": "prettier --write --ignore-unknown",
    "src/**/*.scss": [
      "stylelint --fix",
      "prettier --write --ignore-unknown"
    ]
  }
}
